{
  "manifest_version": 3,
  "name": "FlowTimer Notes",
  "description": "A sleek note & task timer. Add tasks, start/stop timers, and keep history locally.",
  "version": "0.1.0",
  "action": {
    "default_title": "FlowTimer Notes",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "flowtimer-notes@example.com",
      "strict_min_version": "109.0"
    }
  }
}
